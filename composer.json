{
    "name": "elshaden/popup-card",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Elshaden\\PopupCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elshaden\\PopupCard\\CardServiceProvider",
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
